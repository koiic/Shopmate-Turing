{
  "name": "yes",
  "version": "1.0.0",
  "description": "A shopping API with node backend",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha --require @babel/register --timeout 20000 ./src/tests/**/'*.spec.js' --recursive --exit || true",
    "babel-node": "babel-node --presets=env",
    "setup": "node ./setup.js",
    "dev": "cross-env process.env.NODE_ENV=development && npm run lint && nodemon --exec babel-node ./index.js ",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "seed": "cross-env process.env.NODE_ENV=test sequelize db:seed:all",
    "migrate-test": "cross-env process.env.NODE_ENV=test sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate": "sequelize db:migrate",
    "undo": "sequelize db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koiic/Shopmate-Turing.git"
  },
  "keywords": [
    "shopping",
    "turing"
  ],
  "author": "koiic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koiic/Shopmate-Turing/issues"
  },
  "homepage": "https://github.com/koiic/Shopmate-Turing#readme",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.0",
    "sequelize": "^5.8.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2"
  }
}
